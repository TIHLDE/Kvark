import React from 'react';

interface KnappProps {
  tekst: string;
  onClick: () => void;
}

export const NewStudentButton = ({ tekst, onClick }: KnappProps) => {
  return React.createElement('button', { onClick, className: 'bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded' }, tekst);
};
